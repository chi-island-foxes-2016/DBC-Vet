<div class="col-md-4 col-md-offset-4">
<h1>Create a New Document</h1>
<div>
  <%= bootstrap_form_for @document do |f| %>
    <%= f.text_field :title %>
    <%= f.collection_select :group_id, Group.all, :id, :group_type, prompt: true %>
    <%= f.text_area :content, :class => 'document'%>
    <%= f.submit "Create" %>
  <% end %>
</div>
