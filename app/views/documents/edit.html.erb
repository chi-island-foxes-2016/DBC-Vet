<div class="col-md-4 col-md-offset-4">
<h1>Edit this Document</h1>

<div>
  <%= bootstrap_form_for @document, method: :put do |f|%>
    <%= f.text_field :title %>
    <%= f.collection_select :group_id, Group.all, :id, :group_type%>
    <%= f.text_area :content, :class => 'document'%>
    <%= f.submit "Update" %>
  <% end %>
</div>
